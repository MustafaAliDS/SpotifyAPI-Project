{
  "name": "spotifyapiproject",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix",
    "tsc": "tsc --noEmit --emitDeclarationOnly false --experimentalDecorators",
    "tsc:watch": "tsc --noEmit --emitDeclarationOnly false --experimentalDecorators --watch",
    "build": "rimraf dist && npx tsoa spec-and-routes && npm run tsc",
    "predev": "npm run build",
    "dev": "concurrently \"tsc --watch\" \"nodemon -q dist/src/app\"",
    "prestart": "npm run build",
    "test": "cross-env NODE_ENV=test jest --testTimeout=10000 --runInBand --coverage",
    "start": "cross-env NODE_ENV=production node dist/app",
    "tsoa:spec": "npx tsoa spec",
    "tsoa:routes": "npx tsoa routes"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/spotify-api": "^0.0.16",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "https": "^1.0.0",
    "node": "17.7.2",
    "querystring": "^0.2.1",
    "request": "^2.88.2",
    "rimraf": "^3.0.2",
    "swagger-ui-express": "^4.4.0",
    "tsoa": "^4.1.0",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.4",
    "@types/request": "^2.48.8",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "concurrently": "^7.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.18.0",
    "jest": "^28.1.2",
    "nodemon": "^2.0.18",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.5"
  }
}
